                "name": "xinix-technology/bono",
                "version": "dev-master",
                "dist": {
                    "url": "https://github.com/krisanalfa/bono/archive/master.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "xinix-technology/norm",
                "version": "dev-master",
                "dist": {
                    "url": "https://github.com/krisanalfa/norm/archive/master.zip",
                    "type": "zip"
                }
            }
        }
    ],
    "config": {
        "preferred-install": "dist"
    },
    "minimum_stability": "dev"
}
