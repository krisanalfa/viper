{
    "name": "krisanalfa/viper-arch",
    "description": "Viper Archetype",
    "require": {
        "xinix-technology/norm": "dev-master",
        "xinix-technology/bono": "dev-master",

        "filp/whoops": "1.*",
        "reekoheek/util": ">=0.1.2",
        "reekoheek/jsonkit": "~0.0.1",

        "krisanalfa/bono-markdown": "~0.0.2",
        "krisanalfa/bono-blade": "~0.7.2",
        "krisanalfa/blade-theme": "~0.4.6",
        "krisanalfa/bono-kraken": "~0.0.2",

        "reekoheek/bono-auth": "dev-master",

        "nesbot/Carbon": "*",

        "ircmaxell/password-compat": "1.0.*"
    },
    "autoload": {
        "psr-0": {
            "App": "src/",
